* {
  font: "JetBrainsMono NF Medium 20";
  bg: #0011121D;
  bg-none: #aa000000;
  accent: #00e6ac;

  fg: #FFFFFF;
  fg-alt: #666666;

  background-color: @bg;
  
  border: 0;
  margin: 0;
  padding: 0;
  spacing: 0;
}


window {
  width: 45%;
  border-color: @accent;
  border-radius: 10px;
  border: 1px;
}

element {
  padding: 8 0;
  text-color: @fg-alt;
  background-color: @bg-none;
}

element selected {
  text-color: @fg;
}

element-text {
  text-color: inherit;
  vertical-align: 0.5;
  background-color: @bg-none;
}

element-icon {
  size: 34;
  padding: 0 10 0 0;
  background-color: @bg-none;
}

entry {
  background-color: @accent;
  padding: 12;
  // text-color: @fg;
  text-color: black;
}

inputbar {
  children: [prompt, entry];
}

listview {
  padding: 8 12;
  columns: 1;
  lines: 8;
}

mainbox {
  children: [inputbar, listview];
}

prompt {
  background-color: @accent;
  enabled: true;
  padding: 3 0 0 18;
  text-color: black;
  font: "JetBrainsMono NF Medium 30";
}

